{
  "name": "social-media-automation-system",
  "version": "1.0.0",
  "description": "Comprehensive social media automation system integrating WhatsApp Business API, Facebook Graph API, Instagram Business API, and Meta Pixel",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docs": "jsdoc src/ -d docs/jsdoc",
    "swagger": "swagger-jsdoc -d swaggerDef.js src/routes/*.js -o docs/swagger.json"
  },
  "keywords": [
    "whatsapp-business-api",
    "facebook-graph-api",
    "instagram-api",
    "meta-pixel",
    "social-media-automation",
    "nodejs",
    "express"
  ],
  "author": "Assessment Project",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "mongoose": "^8.0.0",
    "joi": "^17.11.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "express-slow-down": "^2.0.1",
    "crypto": "^1.0.1",
    "form-data": "^4.0.0",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jsdoc": "^4.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/assessment/social-media-automation-system.git"
  },
  "bugs": {
    "url": "https://github.com/assessment/social-media-automation-system/issues"
  },
  "homepage": "https://github.com/assessment/social-media-automation-system#readme"
